
public class AudioObject {
static double freq=440;
double sample=0.01;
double amp=1;
int rep = 0;
double mult=1.1666;
void tunein() {
	for (int i=0;i<=3000;i++)
	{
		amp-=0.1;
	}
	StdAudio.play(StdAudio.note(freq, sample, amp));
	rep++;

if (rep<=10000)
{
freq=freq*mult;
}
if (freq>=800)
{
mult=-mult;
freq=0;
}	

if (freq<=30)
{
mult=+1;
}	
}
AudioObject(double frequency, double sampl, double amplitude){

freq=frequency;
sample=sampl;
amp=amplitude;
}

}
