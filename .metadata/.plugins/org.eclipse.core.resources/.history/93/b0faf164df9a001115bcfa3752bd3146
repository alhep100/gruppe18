public class AudioObject {
	static double freq = 220;
	double sample = 0.01;
	double amp = 1;
	int rep = 0;
	double mult = 1;

	void tunein() {

		

		if (freq <= 25000) {
			mult = +mult;
			freq = freq * mult;
			StdAudio.play(StdAudio.note(freq, sample, amp));
			
			amp -= 0.05;
			
		}

		
		
	

		if (freq <= 0) {
			freq = 500;
			StdAudio.play(StdAudio.note(freq, sample, amp));
		}
	}

	AudioObject(double frequency, double sampl, double amplitude) {

		freq = frequency;
		sample = sampl;
		amp = amplitude;
	}

}
