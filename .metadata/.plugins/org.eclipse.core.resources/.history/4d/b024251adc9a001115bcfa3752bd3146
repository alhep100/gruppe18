public class AudioObject {
	static double freq = 440;
	double sample = 0.01;
	double amp = 1;
	int rep = 0;
	double mult = 1;

	void tunein() {
		for (int i = 0; i <= 3000; i++) {
			amp -= 0.1;
		}
		StdAudio.play(StdAudio.note(freq, sample, amp));
		


			freq = freq * mult;
			amp -= 0.05;
			sample+=0.02;
			
	


		if (freq <= 0) {
			freq = 500;
		}
	}

	AudioObject(double frequency, double sampl, double amplitude) {

		freq = frequency;
		sample = sampl;
		amp = amplitude;
	}

}
